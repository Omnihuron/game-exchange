    <%= link_to admin_dashboard_path, class: "item" do %>
      <i class="fa fa-home"></i>
      <label>Overview</label>
    <% end %>
    <%= link_to admin_orders_path, class: "item" do %>
      <i class="fa fa-book"></i>
      <label>View Orders</label>
    <% end %>
    <%= link_to admin_games_path, class: "item" do %>
